{
  "name": "rest-express-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch . --ext ts --exec \"tsx index.ts\"",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/api/index.js",
    "db:push": "drizzle-kit push",
    "db:init": "tsx initializeDatabase.ts",
    "seed:tenants": "tsx seedTenantData.ts",
    "db:reset": "drizzle-kit push --force && npm run seed:tenants",
    "check": "tsc"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.1",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "nanoid": "^5.1.6",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "serverless-http": "^4.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "twilio": "^5.10.3",
    "ws": "^8.18.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.9.1",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pdfkit": "^0.13.5",
    "@types/pg": "^8.11.10",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.1",
    "typescript": "5.6.3"
  }
}
